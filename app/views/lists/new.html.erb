<%= form_with(model: @list, html: { class: "form-list" }) do |form| %>
  <%= form.label :name, "Quel est le nom de cette nouvelle liste?" %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :url, "Avez vous une URL d'image?" %>
  <%= form.text_field :url %>
  <br>
  <%= form.submit "Valider" %>
<% end %>
